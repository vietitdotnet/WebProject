@using WebProject.Extentions.Models;
@model Cache;
@{
    ViewData["Title"] = "Cập nhật bộ nhớ Cache";
    var i = 1;
}

<div class="relative overflow-x-auto"> 
    @if (Model?.ObjCache.Count() > 0)
    {
        <table class="w-full text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400">
            <thead>
                <tr>
                    <th cope="col" class="px-6 py-3">#</th>
                    <th scope="col" class="px-6 py-3">Tên</th>


                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.ObjCache)
                {
                    <tr class="bg-white border-b dark:bg-gray-800 dark:border-gray-700">
                        <th class="px-6 py-4">@i</th>
                        <td class="px-6 py-4">@item.Value</td>
                    </tr>
                    i++;
                }
            </tbody>
        </table>

        <form method="post" asp-action="update">
            <button type="submit" class="mt-5 text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-blue-600 dark:hover:bg-blue-700 focus:outline-none dark:focus:ring-blue-800">Cập nhật</button>
        </form>

    }
    else
    {
        @Html.ActionLink("Quay trở lại trang chủ", "Index")
    }

</div>